pipeline {
	stages {
//-------------------------------------------------- begin stages 

stage('Кубернетес') {
   agent { kubernetes {
         yaml '''
            apiVersion: v1
            kind: Deployment
            spec:
              replicas: 2
            spec:
             containers:
             - name: debian
            image: debian:latest
            '''
			}
		}
        }
//--------------------------------------------------End stages
    }
//--------------------------------------------------End pipline
}