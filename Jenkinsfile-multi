pipeline {
    agent any
	stages {
// begin stages 
stage('1 images') {
            agent {
                docker { image 'debian:latest' }
            }
            steps {
                sh 'uname -a'
            }
        }
stage('2 images') {
            agent {
                docker { image 'debian:latest' }
            }
            steps {
                sh 'uname -a'
            }
        }
stage('3 images') {
            agent {
                docker { image 'debian:latest' }
            }
            steps {
                sh 'uname -a'
            }
        }
        

//End stages
    }
//End pipline
}